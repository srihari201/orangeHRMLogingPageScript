<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Sample Execution">

    <test name="smoke test">
        <groups>
            <run>
                <include name="Smoke"/>
                <!--<exclude name="Sanity" />-->
            </run>
        </groups>
        <classes>
            <class name="com.selnium.practice.TestNGPracrtice.OrangeHRMLoginInvalidCredentials" />
            <class name="com.selnium.practice.TestNGPracrtice.DemoWebShopLoginPage" />
        </classes>
    </test>


    <test name="Sanity test">
        <groups>
            <run>
                <include name="Sanity"/>
            </run>
        </groups>

        <classes>
            <class name="com.selnium.practice.TestNGPracrtice.OrangeHRMLoginInvalidCredentials" />
            <class name="com.selnium.practice.TestNGPracrtice.DemoWebShopLoginPage" />
        </classes>
    </test>

</suite>